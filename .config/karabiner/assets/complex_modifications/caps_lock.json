{
  "title": "My custom keys",
  "rules": [
    {
      "description": "Caps lock to swap tabs",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "caps_lock",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "z",
              "modifiers": [
                "left_control"
              ]
            }
          ]
        }
      ]
    }
  ]
}
